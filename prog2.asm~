lw $8, 0($23)  # load value 0x10000000 into $8
addi $2, $0, 1  # register $2 gets value 1
addi $3, $0, 25  #register $3 gets 25
loop:beq $2, $3, end	
lw $4, 0($8)   # put first memory value into register $4
add $4, $0, $0  #set $4 to zero
sw $4, 0($8)   #set memory location to value 0	
addi $8, $8, 4  #increment address in register $8
addi $2, $2, 1  #increment counter	
j loop
end: add $24, $0, $0


